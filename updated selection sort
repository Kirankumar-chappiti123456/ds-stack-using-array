#include <stdio.h>
int main()
{
    int n=5,i,j,temp;
    int arr[5]={10,22,2,9,11};
    for(i=0;i<n-1;i++)
    {

        for(j=i+1;j<n-1;j++)
        {
            if(arr[i]>arr[j])
            {
                temp=arr[i];
                arr[i]=arr[j];
                arr[j]=temp;
              
            }
        }

    }
    for(i=0;i<n;i++)
    {
        printf("%d\n",arr[i]);
    }
}
