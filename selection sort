int main()
{
    int n,i,j,min=0,temp;
    int a[]={1,4,66,89,3,5,7};
    n=sizeof(a)/sizeof(a[0]);
    for(int i=0;i<n-1;i++)
    {
        int min=i;
        for(int j=i+1;j<n;j++)
        {
            if(a[j]<a[min])
                 min=j;
        }
        
    
    temp=a[i];
        a[i]=a[min];
        a[min]=temp;
}
    for(int i=0;i<n;i++)
    {
        printf(" %d  ",a[i]);
        
    }
}







#include<stdio.h>
int main()
{
    int n,i,j,min=0,temp;
    int a[]={1,4,66,89,3,5,7};
    n=sizeof(a)/sizeof(a[0]);
    for(int i=0;i<n-1;i++)
    {
        int min=i;
        for(int j=i+1;j<n;j++)
        {
            if(a[j]<a[min])
                 min=j;
        }
        if(min!=i)
        {
           temp=a[i];
        a[i]=a[min];
        a[min]=temp;  
        }
        
    
   
}
    for(int i=0;i<n;i++)
    {
        printf(" %d  ",a[i]);
        
    }
}

