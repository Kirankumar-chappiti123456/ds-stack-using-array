array of sort:


#include <stdio.h>

int main()
{
   int a[]={200,1,0,445,222,908},temp;
   int n=sizeof(a)/sizeof(a[0]);
   for(int i=0;i<n;i++)
   {
       for(int j=i+1;j<n;j++)
       {
           if(a[i]>a[j])
           {
               temp=a[i];
               a[i]=a[j];
               a[j]=temp;
           }
       }
   }
   for(int i=0;i<n;i++)
   {
       printf("%d\n",a[i]);
   }
}


selection sort:

#include <stdio.h>

int main()
{
   int a[]={200,1,0,445,222,908},temp;
   int n=sizeof(a)/sizeof(a[0]);
   for(int i=0;i<n;i++)
   {
       for(int j=i+1;j<n;j++)
       {
           if(a[i]>a[j+1])
           {
               temp=a[j];
               a[j]=a[j+1];
               a[j+1]=temp;
           }
       }
   }
   for(int i=0;i<n;i++)
   {
       printf("%d\n",a[i]);
   }
}


array at beg add:



#include <stdio.h>

int main()
{
    int arr[20],n,item;
    printf("enter the size of array:");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    n++;
     printf("enter the new element to insert at begin:");
      scanf("%d",&item);
 for(int i=n;i>1;i--)
 {
     arr[i-1]=arr[i-2];
 }
 arr[0]=item;
 for(int i=0;i<n;i++)
 {
     printf("%d ",arr[i]);
 }
    return 0;
}





array at end  add:



#include <stdio.h>

int main()
{
    int arr[20],n,item;
    printf("enter the size of array:");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    n++;
     printf("enter the new element to insert at begin:");
      scanf("%d",&item);
arr[5]=item;
 for(int i=0;i<n;i++)
 {
     printf("%d ",arr[i]);
 }
    return 0;
}
