array of sort:


#include <stdio.h>

int main()
{
   int a[]={200,1,0,445,222,908},temp;
   int n=sizeof(a)/sizeof(a[0]);
   for(int i=0;i<n;i++)
   {
       for(int j=i+1;j<n;j++)
       {
           if(a[i]>a[j])
           {
               temp=a[i];
               a[i]=a[j];
               a[j]=temp;
           }
       }
   }
   for(int i=0;i<n;i++)
   {
       printf("%d\n",a[i]);
   }
}


